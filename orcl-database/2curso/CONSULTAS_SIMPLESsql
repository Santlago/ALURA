SELECT * FROM TABELA_DE_CLIENTES;

SELECT CPF, NOME, BAIRRO, CIDADE FROM tabela_de_clientes;

SELECT NOME, CPF, CIDADE, BAIRRO FROM tabela_de_clientes;

SELECT CPF AS IDENTIFICADOR, NOME AS "NOME DE CLUENTE", BAIRRO, CIDADE FROM tabela_de_clientes;

SELECT CPF AS IDENTIFICADOR, NOME AS "NOME DE CLUENTE", TDC.BAIRRO, CIDADE FROM tabela_de_clientes TDC;

SELECT TDC.* FROM tabela_de_clientes TDC;