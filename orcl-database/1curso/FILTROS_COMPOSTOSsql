SELECT * FROM AL_TB_PRODUTOS WHERE PRECO_LISTA >= 16 AND PRECO_LISTA <= 21;

SELECT * FROM AL_TB_PRODUTOS WHERE PRECO_LISTA > 16 AND SABOR = 'Laranja';

SELECT * FROM AL_TB_PRODUTOS WHERE PRECO_LISTA > 16 OR SABOR = 'Laranja';

SELECT * FROM AL_TB_PRODUTOS WHERE NOT PRECO_LISTA > 16 OR SABOR = 'Laranja';

SELECT * FROM AL_TB_CLIENTES WHERE (IDADE >= 18 AND SEXO = 'F') AND BAIRRO = 'Jardins';

SELECT * FROM AL_TB_CLIENTES WHERE (IDADE >= 18 AND SEXO = 'F')
AND (CIDADE = 'Sao Paulo' OR BAIRRO = 'Barra da Tijuca');