SELECT * FROM AL_TB_CLIENTES;

SELECT IDADE FROM al_tb_clientes WHERE IDADE = 22;

SELECT IDADE FROM al_tb_clientes WHERE IDADE > 22;

SELECT IDADE FROM al_tb_clientes WHERE IDADE >= 22;

// BUSCANDO TODOS EXCETO QUEM TEM 22 ANOS
SELECT IDADE FROM al_tb_clientes WHERE IDADE <> 22;

SELECT * FROM AL_TB_CLIENTES WHERE NOME > 'Marcelo Matos';

SELECT * FROM AL_TB_CLIENTES WHERE NOME <> 'Marcelo Matos';